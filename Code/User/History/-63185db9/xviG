#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls --color=auto'
alias grep='grep --color=auto'
PS1='[\u@\h \W]\$ '
if [[ $TERM == xterm-kitty ]]; then
    cp "$(~/.config/fastfetch/random_logo.sh)" \
       ~/.config/fastfetch/current_logo.png
fi
fastfetch 
